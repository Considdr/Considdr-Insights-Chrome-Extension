{
  "name": "ConsiddrInsights",
  "version": "1.0",
  "description": "Automatically highlight key insights in an article",
  "permissions": [
      "activeTab",
      "tabs",
      "cookies",
      "storage",
      "http://*/*",
      "https://*/*"
  ],
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
